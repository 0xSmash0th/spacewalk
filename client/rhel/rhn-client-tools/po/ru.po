#: ../src/up2date_client/rhnreg_constants.py:43
msgid "Downloads & Upgrades:"
msgstr "Возможность двухсторонней загрузки:"

#: ../src/up2date_client/rhnreg_constants.py:44
msgid "Support:"
msgstr "Поддержка:"

#: ../src/up2date_client/rhnreg_constants.py:45
msgid "Compliance:"
