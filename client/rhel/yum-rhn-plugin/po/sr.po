#: ../rhnplugin.py:35
msgid "There was an error communicating with RHN."
msgstr "Дошло је до грешке при комуникацији са RHN-ом."

#: ../rhnplugin.py:48
msgid "RHN support will be disabled."
msgstr "RHN подршка ће бити искључена."

#: ../rhnplugin.py:56
msgid "There was an error parsing the RHN proxy settings."
msgstr "Дошло је до грешке при тумачењу подешавања RHN проксија."

#: ../rhnplugin.py:67
msgid "Either --version, or no commands entered"
msgstr "Или --version, или никаква наредба нису унети"

#: ../rhnplugin.py:80
msgid "This system is not registered with RHN."
msgstr "Овај систем није регистрован за RHN."

#: ../rhnplugin.py:85
msgid "RHN channel support will be disabled."
msgstr "Подршка RHN канала ће бити искључена."

#: ../rhnplugin.py:89
msgid "This system is not subscribed to any channels."
msgstr "Систем није претплаћен ни за један канал."

#: ../rhnplugin.py:123
msgid "Package profile information could not be sent."
msgstr "Подаци о профилу пакета нису могли бити послати."

#: ../rhnplugin.py:190
#, python-format
msgid "Missing required login information for RHN: %s"
msgstr "Недостају неопходни подаци за пријаву на RHN: %s"

