#: ../rhnplugin.py:35
msgid "There was an error communicating with RHN."
msgstr "Tijekom komuniciranja s RHM došlo je do pogreške."

#: ../rhnplugin.py:48
msgid "RHN support will be disabled."
msgstr "RHM podrška bit će onemogućena."

#: ../rhnplugin.py:56
msgid "There was an error parsing the RHN proxy settings."
msgstr "Tijekom raščlanjivanja proxy postavki za RHM došlo je do pogreške."

#: ../rhnplugin.py:67
msgid "Either --version, or no commands entered"
msgstr "Nije unesen argument \"--version\" ili ikakva naredba"

#: ../rhnplugin.py:80
msgid "This system is not registered with RHN."
msgstr "Ovaj sustav nije registriran pri RHM."

#: ../rhnplugin.py:85
msgid "RHN channel support will be disabled."
msgstr "RHM podrška za kanale bit će onemogućena."

#: ../rhnplugin.py:89
msgid "This system is not subscribed to any channels."
msgstr "Ovaj sustav nije pretplaćen na ikakav kanal."

#: ../rhnplugin.py:123
msgid "Package profile information could not be sent."
msgstr "Podatke o profilu pakete nije moguće poslati."

#: ../rhnplugin.py:190
#, python-format
msgid "Missing required login information for RHN: %s"
msgstr "Nedostaju potrebno podaci za prijavu pri RHM: %s"

