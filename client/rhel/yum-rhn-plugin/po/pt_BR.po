#: ../rhnplugin.py:35
msgid "There was an error communicating with RHN."
msgstr "Houve um erro ao tentar a comunicação com o RHN."

#: ../rhnplugin.py:48
msgid "RHN support will be disabled."
msgstr "Suporte ao RHN será desabilitado."

#: ../rhnplugin.py:56
msgid "There was an error parsing the RHN proxy settings."
msgstr "Houve um erro ao analisar as configurações do RHN Proxy."

#: ../rhnplugin.py:67
msgid "Either --version, or no commands entered"
msgstr "Ou --version ou nenhum comando foi digitado"

#: ../rhnplugin.py:80
msgid "This system is not registered with RHN."
msgstr "O sistema não está registrado com o RHN."

#: ../rhnplugin.py:85
msgid "RHN channel support will be disabled."
msgstr "Suporte a canais do RHN será desabilitado."

#: ../rhnplugin.py:89
msgid "This system is not subscribed to any channels."
msgstr "Este sistema não está subscrito a nenhum canal."

#: ../rhnplugin.py:123
msgid "Package profile information could not be sent."
msgstr "Informação sobre perfil de pacote não pôde ser enviada."

#: ../rhnplugin.py:190
#, python-format
msgid "Missing required login information for RHN: %s"
msgstr "Informação de login necessária para o RHN não foi encontrada: %s"

