#: ../rhnplugin.py:35
msgid "There was an error communicating with RHN."
msgstr "Terdapat ralat berkomunikasi dengan RHN."

#: ../rhnplugin.py:48
msgid "RHN support will be disabled."
msgstr "Sokongan RHN akan dilumpuhkan."

#: ../rhnplugin.py:56
msgid "There was an error parsing the RHN proxy settings."
msgstr "Terdapat ralat menghurai tetapan proksi RHN."

#: ../rhnplugin.py:67
msgid "Either --version, or no commands entered"
msgstr "Sama ada --version, atau tiada arahan dimasukkan"

#: ../rhnplugin.py:80
msgid "This system is not registered with RHN."
msgstr "Sistem ini tidak berdaftar dengan RHN."

#: ../rhnplugin.py:85
msgid "RHN channel support will be disabled."
msgstr "Sokongan saluran RHN akan dilumpuhkan."

#: ../rhnplugin.py:89
msgid "This system is not subscribed to any channels."
msgstr "Sistem ini tidak melanggan kepada sebarang saluran."

#: ../rhnplugin.py:123
msgid "Package profile information could not be sent."
msgstr "Maklumat profil pakej tidak dapat dihantar."

#: ../rhnplugin.py:190
#, python-format
msgid "Missing required login information for RHN: %s"
msgstr "Maklumat logmasuk untuk RHN yang diperlukan tiada: %s"

