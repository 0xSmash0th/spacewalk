#: ../rhnplugin.py:35
msgid "There was an error communicating with RHN."
msgstr "Došlo je do greške pri komunikaciji sa RHN-om."

#: ../rhnplugin.py:48
msgid "RHN support will be disabled."
msgstr "RHN podrška će biti isključena."

#: ../rhnplugin.py:56
msgid "There was an error parsing the RHN proxy settings."
msgstr "Došlo je do greške pri tumačenju podešavanja RHN proksija."

#: ../rhnplugin.py:67
msgid "Either --version, or no commands entered"
msgstr "Ili --version, ili nikakva naredba nisu uneti"

#: ../rhnplugin.py:80
msgid "This system is not registered with RHN."
msgstr "Ovaj sistem nije registrovan za RHN."

#: ../rhnplugin.py:85
msgid "RHN channel support will be disabled."
msgstr "Podrška RHN kanala će biti isključena."

#: ../rhnplugin.py:89
msgid "This system is not subscribed to any channels."
msgstr "Sistem nije pretplaćen ni za jedan kanal."

#: ../rhnplugin.py:123
msgid "Package profile information could not be sent."
msgstr "Podaci o profilu paketa nisu mogli biti poslati."

#: ../rhnplugin.py:190
#, python-format
msgid "Missing required login information for RHN: %s"
msgstr "Nedostaju neophodni podaci za prijavu na RHN: %s"

