create or replace function rhn_srv_net_iface_mod_trig_fun() returns trigger as
$$
begin
    if new.id is null then
        new.id := nextval('rhn_srv_net_iface_id_seq');
    end if;
    new.modified := current_timestamp;
    return new;
end;
$$ language plpgsql;
