<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.redhat.rhn.domain.iss.IssOrgCatalogue"
        table="rhnsyncorgcatalogue" >
        <id name="id" type="long" column="ID">
            <meta attribute="scope-set">protected</meta>
            <generator class="sequence">
                <param name="sequence">rhn_syncorgcatalogue_seq</param>
            </generator>
        </id>

        <property name="label" column="LABEL"  type="string" length="256" />

        <set name="sourceOrgs" inverse="true" cascade="delete">
            <key column="catalogue_id"/>
            <one-to-many class="com.redhat.rhn.domain.iss.IssSyncOrg"/>
        </set>

    </class>

    <query name="IssOrgCatalogue.findById">
        <![CDATA[from com.redhat.rhn.domain.iss.IssOrgCatalogue as c where c.id = :id]]>
    </query>

    <query name="IssOrgCatalogue.lookupAll">
        <![CDATA[from com.redhat.rhn.domain.iss.IssOrgCatalogue]]>
    </query>

</hibernate-mapping>
