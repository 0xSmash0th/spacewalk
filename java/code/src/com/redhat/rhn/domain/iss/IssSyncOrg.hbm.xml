<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.redhat.rhn.domain.iss.IssSyncOrg"
        table="rhnsyncorgs" >
        <id name="id" type="long" column="ID">
            <meta attribute="scope-set">protected</meta>
            <generator class="sequence">
                    <param name="sequence">rhn_syncorgs_seq</param>
            </generator>
        </id>
        <property name="sourceOrgId" column="SOURCE_ORG_ID"  type="long" />
        <property name="sourceOrgName" column="SOURCE_ORG_NAME"  type="string" length="256" />

        <many-to-one name="catalogue" column="CATALOGUE_ID"  class="com.redhat.rhn.domain.iss.IssOrgCatalogue" />

        <many-to-one name="targetOrg" column="TARGET_ID"  class="com.redhat.rhn.domain.org.Org" />
    </class>
</hibernate-mapping>
