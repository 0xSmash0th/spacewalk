<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.redhat.rhn.domain.rhnpackage.PackageKey" 
        table="rhnPackageKey" mutable="false">
        <cache usage="read-only"/>
        <id name="id" type="long" column="id">
            <meta attribute="scope-set">protected</meta>
            <generator class="sequence">
                <param name="sequence">rhn_pkey_id_seq</param>
            </generator>
        </id>
  
        <property name="key" type="string" column="key"/>
        
        <property name="created" type="date" column="created"/>
        <property name="modified" type="date" column="modified"/>
        
        <many-to-one
            name="type"
            class="com.redhat.rhn.domain.rhnpackage.PackageKeyType"
            column="key_type_id"/>
                    
        <many-to-one
            name="provider"
            class="com.redhat.rhn.domain.rhnpackage.PackageProvider"
            column="package_provider_id"/>
    </class>

    <query name="PackageGpg.findById">
        <![CDATA[from com.redhat.rhn.domain.rhnpackage.PackageKey as p 
        		where p.id = :id]]>
    </query>

    <query name="PackageGpg.findByKey">
        <![CDATA[from com.redhat.rhn.domain.rhnpackage.PackageKey as p 
        		where p.key = :key]]>
    </query>
    
    <query name="PackageGpg.listKeys">
        <![CDATA[from com.redhat.rhn.domain.rhnpackage.PackageKey]]>    	
    </query>
    
 
</hibernate-mapping>
