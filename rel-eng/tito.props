[globalconfig]
default_builder = spacewalkx.builderx.Builder
default_tagger = tito.tagger.VersionTagger
changelog_with_email = 0
changelog_do_not_remove_cherrypick = 0
lib_dir = rel-eng/lib

[spacewalk-nightly-rhel6]
disttag = .el6
blacklist=oracle-server-admin oracle-server-scripts oracle-rhnsat-selinux jabberd-selinux apt-spacewalk python-hwdata mod_perl nodejs-less nodejs scons v8 gyp http-parser libuv c-ares19 nodejs-packaging ycssmin

